print("Enter 2 numbers:")
a=int(input())
b=int(input())
#Addition
print("The sum is:",a+b)
#Subraction
print("The difference is:",a-b)
#Multiplication
print("The product is:",a*b)
#Division
print("The division is:",a/b)
#Modulur Division
print("The remainder is:",a%b)
#Integer Division 
print("The integer Division is:",a//b)